{
	// Place your snippets for handlebars here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"ember freestyle section": {
		"prefix": "freesec",
		"body": [
			"{{#freestyle-section name='${1:name}' as |section|}}",
			"  $0",
			"{{/freestyle-section}}"
		],
		"description": "ember freestyle section"
	},
	"ember freestyle subsection": {
		"prefix": "freesubsec",
		"body": [
			"{{#section.subsection name='${1:name}'}}",
			"  $0",
			"{{/section.subsection}}"
		],
		"description": "ember freestyle subsection"
	},
	"ember freestyle usage": {
		"prefix": "freeusage",
		"body": [
			"{{#freestyle-usage '${1:slug}' title='${2:title}'}}",
			"  $0",
			"{{/freestyle-usage}}"
		],
		"description": "ember freestyle usage"
	},
	"ember freestyle collection": {
		"prefix": "freecol",
		"body": [
			"{{#freestyle-collection title='${1:title}' defaultKey='${2:defaultKey}' as |collection|}}",
			"  $0",
			"{{/freestyle-collection}}"
		],
		"description": "ember freestyle collection"
	},
	"ember freestyle collection variant": {
		"prefix": "freecolvar",
		"body": [
			"{{#collection.variant key='${1:key}'}}",
			"  $0",
			"{{/collection.variant}}"
		],
		"description": "ember freestyle collection variant"
	},
}
