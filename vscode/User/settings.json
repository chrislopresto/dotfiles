// Place your settings in this file to overwrite the default settings
{
  "editor.fontFamily": "Monoid",
  "terminal.external.osxExec": "iTerm.app",
  "editor.fontLigatures": true,
  "editor.lineHeight": 20,
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    140
  ],
  "editor.tabSize": 2,
  "editor.quickSuggestionsDelay": 1,
  "emmet.includeLanguages": {
    "erb": "html",
    "twig": "html",
    "mdx": "Markdown React (mdx)"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "workbench.iconTheme": "vscode-icons",
  "workbench.sideBar.location": "right",
  "window.zoomLevel": -1,
  "workbench.colorTheme": "Framer Syntax",
  "workbench.statusBar.visible": false,
  "workbench.activityBar.visible": false,
  "vsicons.projectDetection.autoReload": true,
  "vsicons.dontShowNewVersionMessage": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": true,
    "suppressWelcomeNotice": true
  },
  "git.enableCommitSigning": true,
  "workbench.editor.showTabs": true,
  "gitlens.gitExplorer.files.layout": "list",
  "vscodeWorkspaceSwitcher.paths": [
    "/Users/chrislopresto/.dotfiles/vscode/workspaces",
    "/Users/chrislopresto/.dotfiles/vscode/workspaces/private"
  ],
  "files.associations": {
    ".env.sample": "dotenv",
    "*.css": "postcss"
  },
  "gitlens.keymap": "chorded",
  "gitlens.historyExplorer.enabled": true,
  "vscodeWorkspaceSwitcher.codeExecutable": "/usr/local/bin/code",
  "vscodeWorkspaceSwitcher.codeInsidersExecutable": "/usr/local/bin/code-insiders",
  "breadcrumbs.enabled": true,
  "editor.cursorBlinking": "expand",
  "githubPullRequests.hosts": [
    {
      "host": "https://github.com",
      "username": "oauth",
      "token": "system"
    }
  ],
  "window.nativeTabs": true,
  "window.nativeFullScreen": false,
  "bracket-pair-colorizer-2.forceIterationColorCycle": true,
  "editor.fontSize": 14,
  "terminal.integrated.fontFamily": "Iosevka"
}
