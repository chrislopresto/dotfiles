// Place your key bindings in this file to overwrite the defaults
[
  { "key": "ctrl+h", "command": "workbench.action.focusPreviousGroup" },
  { "key": "ctrl+l", "command": "workbench.action.focusNextGroup" },
  { "key": "ctrl+1", "command": "workbench.action.focusFirstEditorGroup" },
  { "key": "ctrl+2", "command": "workbench.action.focusSecondEditorGroup" },
  { "key": "ctrl+3", "command": "workbench.action.focusThirdEditorGroup" },
  { "key": "ctrl+4", "command": "workbench.action.focusPanel" },
  { "key": "cmd+k cmd+z", "command": "workbench.action.maximizeEditor" },
  { "key": "cmd+k cmd+shift+z", "command": "workbench.action.evenEditorWidths" },
  { "key": "cmd+\\", "command": "workbench.action.toggleSidebarVisibility" },
]
