[user]
	name = Stevie James
	email = djsteviejames@hotmail.com

[credential]
  helper = osxkeychain

[color]
  ui = true

[core]
	editor = subl -n -w
  excludesfile = ~/.gitignore

[mergetool]
  keepBackup = true

[push]
  default = simple

[filter "media"]
  required = true
	clean = git media clean %f
	smudge = git media smudge %f
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
